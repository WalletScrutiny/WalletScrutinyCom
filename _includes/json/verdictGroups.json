{% assign verdicts = '' %}

{% assign verdictGroups = site.data.topVerdicts | concat: site.data.verdictGroups %}

{% for vg in verdictGroups %}
  {% assign vgJson = vg | jsonify %}
  {% assign verdicts = verdicts | append: '"' | append: vg.key | append: '": '
    | append: vgJson %}
  {% unless forloop.last %}
    {% assign verdicts = verdicts | append: ',' %}
  {% endunless %}
{% endfor %}
const verdictGroups = { {{verdicts}} }
